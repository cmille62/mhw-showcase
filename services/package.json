{
  "name": "mhw-showcase-services",
  "version": "1.0.0",
  "description": "Services for the MHW Showcase",
  "main": "dist/index.js",
  "author": "Clayton Miller <clayton@milleracehardware.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "output-version": "genversion --es6 --semi ./src/version.ts && eslint ./src/version.ts --fix",
    "clean": "shx rm -rf _bundles dist dist-esm",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "yarn run clean",
    "build": "tsc && tsc -m es6 --outDir dist-esm",
    "start": "node ./dist/index.js",
    "dev": "yarn build && yarn start",
    "test:unit": "jest",
    "lint": "eslint './src/**/*.ts'"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "body-parser-xml": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.10.6",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/body-parser-xml": "^1.1.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.36",
    "@types/node": "14.14.31",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^26.6.3",
    "prettier": "^2.0.4",
    "shx": "^0.3.2",
    "ts-jest": "^26.2.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2"
  }
}
