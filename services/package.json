{
  "name": "mhw-showcase-services",
  "version": "1.0.0",
  "description": "Services for the MHW Showcase",
  "main": "dist/index.js",
  "author": "Clayton Miller <clayton@milleracehardware.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "shx rm -rf _bundles dist dist-esm",
    "tslint": "tslint -c tslint.json -p tsconfig.json --fix",
    "prebuild": "yarn run clean",
    "build": "tsc && tsc -m es6 --outDir dist-esm",
    "start": "node ./dist/index.js",
    "dev": "yarn build && yarn start",
    "test:unit": "jest"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "body-parser-xml": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "lodash": "^4.17.15",
    "mongoose": "^5.10.6",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@types/body-parser-xml": "^1.1.0",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/jest": "^26.0.14",
    "@types/lodash": "^4.14.149",
    "@types/mongoose": "^5.7.36",
    "@types/node": "14.14.31",
    "jest": "^26.4.2",
    "shx": "^0.3.2",
    "tslint": "^5.20.0",
    "ts-jest": "^26.2.0",
    "typescript": "^3.6.4"
  }
}
